{
    "compilerOptions": {
        "baseUrl": "./",
        "outDir": "./dist/",
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "paths": {
            "*": ["./src/*"]
        }
    },
    "include": ["./src", "./assets"]
}
